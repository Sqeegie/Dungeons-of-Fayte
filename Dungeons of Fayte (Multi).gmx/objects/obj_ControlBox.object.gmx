                draw_text(x+xline,y+yline,'8456: MOVE#[*]: ATTACK#[-]: SPECIAL');
                break;
            case 6:
                draw_text(x+xline,y+yline,'IJKL: MOVE#[T]: ATTACK#[Y]: SPECIAL');
                break;
            case 100:
                draw_text(x+xline,y+yline,'ARROWS: MOVE#[Z]: ATTACK#[X]: SPECIAL');
                break;
