            <kind>1</kind>
            <string>if(deadCount &gt;= ds_list_size(myMonsters) &amp;&amp; pause == 0) {
    var templevel,tempwave;
    wave +=1;
    
    templevel = round(wave div 10);
    tempwave = wave mod 10;
    
    deadCount = 0;
    ds_list_clear(myMonsters);
    
    switch(tempwave) {
        case 0:
            monsterA = obj_DeathEnemy;
            monsterANum = 0;
            monsterB = obj_BSlime;
            monsterBNum = -1;
            break;
        case 1:
            monsterA = obj_GSoul;
            monsterANum = 4;
            monsterB = obj_BSoul;
            monsterBNum = 2;
            break;
        case 2:
            monsterA = obj_MummyK;
            monsterANum = 1;
            monsterB = obj_Mummy;
            monsterBNum = 2;
            break;
        case 3:
            monsterA = obj_PBlob;
            monsterANum = 6;
            monsterB = obj_GBlob;
            monsterBNum = 3;
            break;
        case 4:
            monsterA = obj_Lich;
            monsterANum = 1;
            monsterB = obj_WBlob;
            monsterBNum = 2;
            break;
        case 5:
            monsterA = obj_QSlime;
            monsterANum = 6;
            monsterB = obj_SkelWarrior;
            monsterBNum = 3;
            break;
        case 6:
            monsterA = obj_SkelCaptain;
            monsterANum = 2;
            monsterB = obj_Wraith;
            monsterBNum = 2;
            break;
        case 7:
            monsterA = obj_RSoul;
            monsterANum = 4;
            monsterB = obj_BFlayer;
            monsterBNum = 2;
            break;
        case 8:
            monsterA = obj_Demon;
            monsterANum = 1;
            monsterB = obj_YFlayer;
            monsterBNum = 2;
            break;
        case 9:
            monsterA = obj_Shadow;
            monsterANum = 1;
            monsterB = obj_BSlime;
            monsterBNum = 4;
            break; 
    }
    
    monsterANum += templevel;
    monsterBNum += templevel;
    
    var flip, monsterAQuant, monsterBQuant;
    flip = 0;
    monsterAQuant = 0;
    monsterBQuant = 0;
    
    for (i=0; i&lt;monsterANum+monsterBNum; i+=1) {
        var xpos, ypos;
        
        xpos = ds_list_find_value(spawnxlist,(i mod ds_list_size(spawnxlist)));
        ypos = ds_list_find_value(spawnylist,(i div ds_list_size(spawnxlist)));
        
        if(flip == 0) {
            if(monsterAQuant &lt; monsterANum) {
                Monster(monsterA,xpos,ypos);
                monsterAQuant +=1;
            }
            else {
                Monster(monsterB,xpos,ypos);
                monsterBQuant +=1;
            }
                
            flip = 1;
        }
        else {
            if(monsterBQuant &lt; monsterBNum) {
                Monster(monsterB,xpos,ypos);
                monsterBQuant +=1;
            }
            else {
                Monster(monsterA,xpos,ypos);
                monsterAQuant +=1;
            }
            
            flip = 0;
        }
    }
    
    alarm[0] = 30;
}



